{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üöÄ TKA Launcher",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/launcher/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/src;${workspaceFolder}/launcher;${workspaceFolder}/packages;${workspaceFolder}/.venv/Lib/site-packages",
        "VIRTUAL_ENV": "${workspaceFolder}/.venv",
        "PYTHONNOUSERSITE": "1"
      },
      "stopOnEntry": false,
      "showReturnValue": true,
      "subProcess": true
    },
